import random 

print("Welcome to your fortune teller!") 
print("If you’re lucky, you’ll have a lucky outcome.")

magic = random.randrange(0,100)

input("pick a number")

if(magic <=5):
  print(" Unlucky! You will get stuck in the rain")

elif(magic ==25):
  print("Unlucky! you will crack your phone")
 
elif(magic >25):
  print(" Lucky! you will win 1 million dollars ")

elif(magic == 100):
  print("You won the lottery! You will get a free trip to the moon")
